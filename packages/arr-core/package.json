{
  "name": "@allrightsrespected/sdk",
  "version": "0.1.1",
  "description": "ARR core protocol library (canonicalization, signing, verification, adapters).",
  "license": "CC0-1.0",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "arr",
    "attribution",
    "cryptography",
    "ed25519",
    "metadata"
  ],
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kanoliban/All-Rights-Respected.git",
    "directory": "packages/arr-core"
  },
  "bugs": {
    "url": "https://github.com/kanoliban/All-Rights-Respected/issues"
  },
  "homepage": "https://www.allrightsrespected.com/sdk",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "pack:check": "npm_config_cache=../../.npm-cache npm pack --dry-run"
  }
}
